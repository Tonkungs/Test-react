{
  "name": "react-own",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  },
  "license": "MIT",
  "scripts": {
    "p": "prettier --write 'src/**/*.js'",
    "pw": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "lint:fix": "yarn run lint --fix",
    "check-staged": "lint-staged",
    "prettier": "./node_modules/.bin/prettier --write src/*.js",
    "dev": "webpack-dev-server --mode development --open --config webpack/webpack.dev.js",
    "build": "webpack --mode production --config webpack/webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@gfx/zopfli": "^1.0.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-react-hot-loader": "^3.0.0-beta.6",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "moment-locales-webpack-plugin": "^1.0.7",
    "node-sass": "^4.9.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.10.0",
    "element-react": "^1.4.21",
    "element-theme-default": "^1.4.13",
    "formik": "^1.3.1",
    "install": "^0.12.1",
    "moment": "^2.22.2",
    "onchange": "^5.0.2",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-redux-form": "^1.16.10",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-calendar-react": "^0.11.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0",
    "standard": "^12.0.1",
    "yup": "^0.26.6"
  }
}
